{
  "name": "async-describe",
  "version": "1.2.0",
  "description": "function for running asynchronous tests",
  "main": "lib/async-describe.js",
  "module": "es/async-describe.js",
  "jsnext:main": "es/async-describe.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "docs"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "prepublishOnly": "npm run test:all",
    "test:all": "npm run clean && npm run lint && npm run test && npm run build ",
    "test:update:snapshot": "npm run test -- --updateSnapshot",
    "clean": "rimraf coverage dist es lib",
    "demo": "node demo",
    "test": "DEMO_DELAY=0 NODE_ENV=test jest"
  },
  "keywords": [
    "describe",
    "promise",
    "async",
    "await"
  ],
  "author": "Guillaume ARM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guillaumearm/async-describe"
  },
  "dependencies": {
    "cli-color": "^1.2.0",
    "clui": "^0.3.6",
    "ramda": "^0.25.0",
    "std-mocks": "^1.0.1",
    "terminal-kit": "^1.15.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-node": "^5.2.1",
    "jest": "^21.2.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "engines": {
    "node": ">=7.10.0"
  }
}
